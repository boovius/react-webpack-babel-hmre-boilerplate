{
  "name": "react-webpack-babel-hmre-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate for react, webpack, babel, es2015, mocha, and hot mod replacement",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/boovius/react-webpack-babel-hmre-boilerplate.git"
  },
  "main": "index.js",
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-register"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "es2015",
    "hot module replacement",
    "mocha",
    "express",
    "boilerplate"
  ],
  "author": "boovius",
  "license": "ISC"
